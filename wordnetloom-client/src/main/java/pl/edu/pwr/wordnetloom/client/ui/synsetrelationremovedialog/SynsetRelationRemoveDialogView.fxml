<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.CheckListView?>

<VBox prefHeight="350.0" prefWidth="500.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.pwr.wordnetloom.client.ui.synsetrelationremovedialog.SynsetRelationRemoveDialogView">
    <CheckListView fx:id="relationListView" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin></CheckListView>
    <ButtonBar minHeight="40.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" style="-fx-background-color: #dddddd; -fx-border-color: #777777; -fx-border-width: 1 0 1 0;" VBox.vgrow="NEVER">
        <buttons>
            <Button fx:id="selectAllButton" defaultButton="true" mnemonicParsing="false" onAction="#selectAll" text="%remove.synset.relation.select.all"/>
            <Button fx:id="deselectAllButton" defaultButton="true" mnemonicParsing="false" onAction="#deselectAll" text="%remove.synset.relation.unselect.all" />
        </buttons>
        <buttons>
            <Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" onAction="#ok" text="%common.ok" />
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#close" text="%common.close" />
        </buttons>
        <padding>
            <Insets left="15.0" right="15.0" />
        </padding>
    </ButtonBar>
</VBox>
