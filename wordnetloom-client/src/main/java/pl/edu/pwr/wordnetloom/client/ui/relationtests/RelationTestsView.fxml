<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.edu.pwr.wordnetloom.client.ui.relationtests.RelationTestsView">
   <HBox VBox.vgrow="ALWAYS">
   <children>
      <ListView fx:id="testsListView" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <VBox maxWidth="35.0" minWidth="35.0" spacing="2.0" HBox.hgrow="NEVER">
         <children>
            <!--<Button fx:id="testUpButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" />
            <Button fx:id="testDownButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" />-->
            <Button fx:id="addTestButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#add"/>
            <Button fx:id="editTestButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#edit"/>
            <Button fx:id="deleteTestButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#delete"/>
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </VBox>
   </children>
   </HBox>
</VBox>
