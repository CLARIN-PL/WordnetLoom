<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TitledPane animated="false" minHeight="300.0" text="%sense.relations.title" xmlns="http://javafx.com/javafx/8.0.181"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.pwr.wordnetloom.client.ui.senserelations.SenseRelationsView">
<content>
   <VBox spacing="2.0">
      <children>
         <TreeView fx:id="relationTree" maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
         <HBox alignment="CENTER" spacing="2.0">
            <children>
               <Button fx:id="addButton" mnemonicParsing="false" onAction="#addSenseRelation"/>
               <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeSenseRelation"/>
            </children>
         </HBox>
      </children>
   </VBox>
</content>
</TitledPane>
