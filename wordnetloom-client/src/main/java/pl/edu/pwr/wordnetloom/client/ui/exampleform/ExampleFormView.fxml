<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<VBox prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
	  fx:controller="pl.edu.pwr.wordnetloom.client.ui.exampleform.ExampleFormView">
	<Label maxWidth="1.7976931348623157E308" style="-fx-border-color: #dddddd; -fx-border-width: 1 0 1 0; -fx-background-color: linear-gradient(from 25% 25% to 100% 100%, #f3f3f3, #fafafa);" text="Use case">
		<padding>
			<Insets bottom="2.0" left="10.0" right="2.0" top="2.0" />
		</padding>
		<font>
			<Font name="System Bold Italic" size="11.0" />
		</font>
		<VBox.margin>
			<Insets bottom="5.0" top="5.0" />
		</VBox.margin>
	</Label>
   <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
      <content>
         <TextArea fx:id="example" maxWidth="-Infinity" wrapText="true" />
      </content>
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
   </ScrollPane>
</VBox>
